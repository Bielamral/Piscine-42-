/* #include <unistd.h> */

/* void	ft_putchar(char c) */
/* { */
/* 	write(1, &c, 1); */
/* } */

/* void	helper(int n) */
/* { */
/* 	int	a; */
/* 	int	b; */

/* 	a = n / 10; */
/* 	b = n % 10; */
/* 	ft_putchar(a + '0'); */
/* 	ft_putchar(b + '0'); */
/* } */

/* void	ft_print_comb2(void) */
/* { */
/* 	int	a; */
/* 	int	b; */

/* 	a = 0; */
/* 	while (a <= 98) */
/* 	{ */
/* 		b = a + 1; */
/* 		while (b <= 99) */
/* 		{ */
/* 			helper(a); */
/* 			ft_putchar(' '); */
/* 			helper(b); */
/* 			if (!(a == 98 && b == 99)) */
/* 			{ */
/* 				ft_putchar(','); */
/* 				ft_putchar(' '); */
/* 			} */
/* 			b++; */
/* 		} */
/* 		a++; */
/* 	} */
/* } */
/* /\* */
